<p id="notice"><%= notice %></p>

<h1>Marketing Teams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Coolness</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marketing_teams.each do |marketing_team| %>
      <tr>
        <td><%= marketing_team.name %></td>
        <td><%= marketing_team.coolness %></td>
        <td><%= link_to 'Show', marketing_team %></td>
        <td><%= link_to 'Edit', edit_marketing_team_path(marketing_team) %></td>
        <td><%= link_to 'Destroy', marketing_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Marketing Team', new_marketing_team_path %>
